{
  "llama-3.2-1b-instruct": {
    "engine": "transformers",
    "modelName": "Llama-3.2-1b-Instruct",
    "modelType": "text-generation",
    "repo": "Xenova/llama-3.2-1b-instruct",
    "pipeline": "text-generation",
    "defaultParams": {
      "temperature": 0.7,
      "maxLength": 2048
    },
    "defaultQuantization": "q4"
  },
  "whisper-tiny-en": {
    "engine": "transformers",
    "modelName": "whisper-tiny-en",
    "modelType": "automatic-speech-recognition",
    "repo": "onnx-community/whisper-tiny.en",
    "pipeline": "automatic-speech-recognition",
    "defaultQuantization": "q4"
  },
  "speecht5-tts": {
    "engine": "transformers",
    "modelName": "speecht5_tts",
    "modelType": "text-to-speech",
    "repo": "Xenova/speecht5_tts",
    "pipeline": "text-to-speech",
    "defaultQuantization": "none",
    "defaultParams": {
      "speaker_embeddings": "https://huggingface.co/datasets/Xenova/transformers.js-docs/resolve/main/speaker_embeddings.bin"
    }
  }
} 
